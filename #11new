#include<bits/stdc++.h>
using namespace std;
int main(){
    int t;
    cin>>t;
    int n;
    while(t--){
        cin>>n;
        int p;
        p=n/5;
        long long int sum=0;
        for(int i=1;i<13;i++){
            long long int s=5*pow(5,i);
            if(s<=n){
                sum+=n/s;}}
            
            sum+=p;
            cout<<sum<<endl;
        
    }
  
}