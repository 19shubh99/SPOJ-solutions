#include<bits/stdc++.h>
using namespace std;
int main(){
   float a,b,c;
   cin>>a>>b>>c;
   while(a!=0||b!=0||c!=0){
       if(b==(a+c)/2.0){
           cout<<"AP"<<" "<<(((2.0)*c)-b)<<endl;
       }
       else if(b*b==a*c){
           cout<<"GP"<<" "<<(b*c)/a<<endl;
       }
       cin>>a>>b>>c;
       }
  
}